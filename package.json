{
  "name": "lumina-photo-gallery",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:workers && next build",
    "build:workers": "tsc scripts/blurhash-worker-only.ts scripts/thumbnail-worker-only.ts --outDir scripts --module commonjs --target es2017 --moduleResolution node --esModuleInterop --allowSyntheticDefaultImports --skipLibCheck",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:generate:prod": "dotenv -e .env.production -- prisma generate",
    "db:push": "prisma db push",
    "db:push:prod": "dotenv -e .env.production -- prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:studio:prod": "dotenv -e .env.production -- prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:prod": "dotenv -e .env.production -- tsx prisma/seed.ts",
    "db:migrate-to-mariadb": "tsx scripts/migrate-data.ts",
    "db:migrate:sqlite-to-mariadb": "node scripts/migrate-sqlite-to-mariadb.js",
    "db:migrate:sqlite-to-mariadb:dry-run": "node scripts/migrate-sqlite-to-mariadb.js --dry-run",
    "setup:production": "bash scripts/setup-production.sh",
    "setup:prisma-prod": "bash scripts/setup-prisma-production.sh",
    "setup:database-url": "bash scripts/construct-database-url.sh",
    "debug:env": "bash scripts/debug-env.sh",
    "worker:thumbnails:all": "tsx scripts/thumbnail-queue-worker-all.ts",
    "worker:blurhash": "tsx scripts/blurhash-worker.ts",
    "worker:exif": "tsx scripts/exif-worker.ts",
    "worker:sync": "tsx scripts/sync-worker.ts",
    "docker:build:prod": "docker compose -f docker-compose.prod.yml build --parallel",
    "docker:build:prod:fast": "docker compose -f docker-compose.prod.yml build --parallel --no-cache-filter builder",
    "docker:up:prod": "docker compose -f docker-compose.prod.yml up -d",
    "worker:uploads": "tsx scripts/upload-worker.ts",
    "worker:all": "concurrently \"npm:worker:uploads\" \"npm:worker:thumbnails:all\" \"npm:worker:exif\" \"npm:worker:sync\" \"npm:worker:blurhash\"",
    "seed:admin": "tsx scripts/create-initial-admin.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.696.0",
    "@aws-sdk/lib-storage": "^3.696.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@bull-board/express": "^6.12.7",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@types/adm-zip": "^0.5.7",
    "@types/fluent-ffmpeg": "^2.1.27",
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "blurhash": "^2.0.5",
    "bull": "^4.16.5",
    "bullmq": "^5.34.0",
    "canvas": "^3.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "exifr": "^7.1.3",
    "fluent-ffmpeg": "^2.1.3",
    "ioredis": "^5.4.1",
    "lucide-react": "^0.468.0",
    "mysql2": "^3.14.3",
    "next": "^15.5.0",
    "next-auth": "^4.24.10",
    "next-intl": "^4.3.4",
    "pgvector": "^0.2.1",
    "prisma": "^6.1.0",
    "react": "^19.0.0",
    "react-blurhash": "^0.3.0",
    "react-dom": "^19.0.0",
    "redis": "^5.8.2",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "better-sqlite3": "^12.2.0",
    "eslint": "^9.18.0",
    "eslint-config-next": "^15.5.0",
    "postcss": "^8.5.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  }
}
